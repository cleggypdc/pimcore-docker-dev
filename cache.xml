<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
    <backend>
        <type>\Pimcore\Cache\Backend\Redis2</type>
        <custom>true</custom>
        <options>
            <server>127.0.0.1</server>
            <port>6379</port>
            <persistent>1</persistent>
            <database>1</database>
            <use_lua>1</use_lua>
        </options>
    </backend>
</zend-config>
