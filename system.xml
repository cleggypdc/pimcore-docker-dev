<?xml version="1.0"?>
<zend-config xmlns:zf="http://framework.zend.com/xml/zend-config-xml/1.0/">
    <general>
        <timezone>Europe/London</timezone>
        <php_cli/>
        <domain/>
        <redirect_to_maindomain/>
        <language>en</language>
        <validLanguages>en,de</validLanguages>
        <fallbackLanguages>
            <en/>
            <de/>
        </fallbackLanguages>
        <theme/>
        <contactemail/>
        <loginscreencustomimage/>
        <disableusagestatistics/>
        <debug>1</debug>
        <debug_ip/>
        <http_auth>
            <username/>
            <password/>
        </http_auth>
        <custom_php_logfile>1</custom_php_logfile>
        <debugloglevel>debug</debugloglevel>
        <debug_admin_translations/>
        <devmode/>
        <logrecipient/>
        <viewSuffix/>
        <instanceIdentifier/>
        <show_cookie_notice/>
        <extjs6>1</extjs6>
    </general>
    <database>
        <adapter>Pdo_MySql</adapter>
        <params>
            <host>localhost</host>
            <username>pimcore_dev</username>
            <password>pimcore_dev_password</password>
            <dbname>pimcore_dev</dbname>
            <port>3306</port>
        </params>
    </database>
    <documents>
        <versions>
            <days/>
            <steps>10</steps>
        </versions>
        <default_controller>default</default_controller>
        <default_action>default</default_action>
        <error_pages/>
        <createredirectwhenmoved/>
        <allowtrailingslash>no</allowtrailingslash>
        <allowcapitals>no</allowcapitals>
        <generatepreview/>
        <wkhtmltoimage/>
        <wkhtmltopdf/>
    </documents>
    <objects>
        <versions>
            <days/>
            <steps>10</steps>
        </versions>
    </objects>
    <assets>
        <webdav>
            <hostname/>
        </webdav>
        <versions>
            <days/>
            <steps>10</steps>
        </versions>
        <ffmpeg/>
        <ghostscript/>
        <libreoffice/>
        <icc_rgb_profile/>
        <icc_cmyk_profile/>
    </assets>
    <services>
        <translate>
            <apikey/>
        </translate>
        <google/>
    </services>
    <cache>
        <enabled/>
        <lifetime/>
        <excludePatterns/>
        <excludeCookie/>
    </cache>
    <outputfilters>
        <less/>
        <lesscpath/>
    </outputfilters>
    <webservice>
        <enabled/>
    </webservice>
    <httpclient>
        <adapter>Zend_Http_Client_Adapter_Socket</adapter>
        <proxy_host/>
        <proxy_port/>
        <proxy_user/>
        <proxy_pass/>
    </httpclient>
    <email>
        <sender/>
        <return/>
        <method>sendmail</method>
        <smtp>
            <host/>
            <port/>
            <ssl/>
            <name/>
            <auth>
                <method/>
                <username/>
                <password/>
            </auth>
        </smtp>
        <debug/>
        <bounce>
            <type/>
            <maildir/>
            <mbox/>
            <imap>
                <host/>
                <port/>
                <username/>
                <password/>
                <ssl/>
            </imap>
        </bounce>
    </email>
    <newsletter>
        <sender>
            <name/>
            <email/>
        </sender>
        <return>
            <name/>
            <email/>
        </return>
        <method/>
        <smtp>
            <host/>
            <port/>
            <ssl/>
            <name/>
            <auth>
                <method/>
                <username/>
                <password/>
            </auth>
        </smtp>
        <usespecific/>
    </newsletter>
</zend-config>
